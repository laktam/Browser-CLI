{
    "manifest_version": 3,
    "name": "Browser CLI",
    "version": "1.0",
    "description": "a CLI for the browser",
    "permissions": [
        "activeTab",
        "tabs",
        "tabGroups"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "./popup/popup.html"
    },
    "chrome_url_overrides" : {
        "newtab": "./new-tab/index.html"
    },
    "content_security_policy": {
        "extension_pages": "default-src 'self'; script-src 'self'; object-src 'none'; img-src 'self' https://example.com; style-src 'self';"
    }
}