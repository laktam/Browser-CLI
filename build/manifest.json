{
    "manifest_version": 3,
    "name": "Browser CLI",
    "version": "1.0",
    "description": "a CLI for the browser",
    "permissions": [
        "activeTab",
        "tabs",
        "commands",
        "tabGroups"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "./popup/popup.html"
    },
    "chrome_url_overrides" : {
        "newtab": "./new-tab/index.html"
    }
}